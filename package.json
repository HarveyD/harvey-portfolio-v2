{
  "name": "seng-portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts-ts": "2.17.0"
  },
  "keywords": [
    "portfolio",
    "personal",
    "website",
    "software",
    "engineer",
    "developer"
  ],
  "author": "Harvey Delaney",
  "license": "MIT",
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "prerender": "react-snap",
    "special-build": "benny-hill npm run build"
  },
  "homepage": "http://www.harveydelaney.com",
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.10.2",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "benny-hill": "1.3.0",
    "font-awesome": "^4.7.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react-snap": "^1.19.0",
    "typescript": "^3.0.3"
  }
}
